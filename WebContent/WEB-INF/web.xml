<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>BatalhaMoral</display-name>
  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>launch.Main</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Index</servlet-name>
    <servlet-class>launch.Index</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Adicionar</servlet-name>
    <servlet-class>servlet.Adicionar</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BatalhaServlet</servlet-name>
    <servlet-class>servlet.BatalhaServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CandidatoServlet</servlet-name>
    <servlet-class>servlet.CandidatoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UsuarioServlet</servlet-name>
    <servlet-class>servlet.UsuarioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BatalhaServlet</servlet-name>
    <url-pattern>/batalha</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidatoServlet</servlet-name>
    <url-pattern>/candidato</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CandidatoServlet</servlet-name>
    <url-pattern>/candidato/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UsuarioServlet</servlet-name>
    <url-pattern>/usuario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Adicionar</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>UrlRewriteFilter</filter-name>
    <filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>UrlRewriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
  </welcome-file-list>
</web-app>